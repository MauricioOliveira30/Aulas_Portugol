programa
{
	
	funcao inicio()
	{
	caracter resp='S'
	inteiro n
	faca{
		escreva("Digite um número:")
		leia(n)
		escreva("Quer continuar? [S/N]")
          leia(resp)
	}enquanto(resp=='S')
		
	}
		
	
}
